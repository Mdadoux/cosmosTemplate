{% extends 'admin/admin.html.twig' %}
{% block title %}Hello AdminClientController!{% endblock %}

{% block body %}

<div class="example-wrapper">
    <h1>List des clients</h1>
    <div class="heading">
        <a href="{{ path('client.create') }}">Ajouter nouveau client</a>
    </div>
    <ul>
  {% for client in listClients %}
      <li><a href="{{ path('client.edite',{id:client.id}) }}">{{ client.name }}</a></li>
    {% endfor %}
    </ul>
</div>
{% endblock %}
